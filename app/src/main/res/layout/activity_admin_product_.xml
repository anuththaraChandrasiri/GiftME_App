<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FEFAC0"
    tools:context=".AddProduct_Activity">

    <TextView
        android:id="@+id/TVNavBarAdmin"
        android:layout_width="400dp"
        android:layout_height="70dp"
        android:background="#FFC957"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.47"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.995" />

    <ImageButton
        android:id="@+id/btnAdminHome"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#FFC957"
        app:layout_constraintBottom_toBottomOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toStartOf="@+id/btnAdminOrders"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="@+id/TVNavBarAdmin"
        app:layout_constraintTop_toTopOf="@+id/TVNavBarAdmin"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home" />

    <ImageButton
        android:id="@+id/btnAdminOrders"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#FFC957"
        app:layout_constraintBottom_toBottomOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toStartOf="@+id/btnAdminProducts"
        app:layout_constraintTop_toTopOf="@+id/TVNavBarAdmin"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/cart" />

    <ImageButton
        android:id="@+id/btnAdminProducts"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginEnd="52dp"
        android:backgroundTint="#FFC957"
        app:layout_constraintBottom_toBottomOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toStartOf="@+id/btnAdminAccount"
        app:layout_constraintTop_toTopOf="@+id/TVNavBarAdmin"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/occasion" />

    <ImageButton
        android:id="@+id/btnAdminAccount"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginEnd="36dp"
        android:backgroundTint="#FFC957"
        app:layout_constraintBottom_toBottomOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toEndOf="@+id/TVNavBarAdmin"
        app:layout_constraintTop_toTopOf="@+id/TVNavBarAdmin"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/account" />

    <TextView
        android:id="@+id/TvNavAdHome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/view_home"
        android:textColor="#0B0909"
        app:layout_constraintBottom_toBottomOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toStartOf="@+id/btnAdminOrders"
        app:layout_constraintHorizontal_bias="0.455"
        app:layout_constraintStart_toStartOf="@+id/TVNavBarAdmin"
        app:layout_constraintTop_toBottomOf="@+id/btnAdminHome"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/TVNavAdOrder"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/View_orders"
        android:textColor="#100E0E"
        app:layout_constraintBaseline_toBaselineOf="@+id/TvNavAdHome"
        app:layout_constraintEnd_toStartOf="@+id/btnAdminProducts"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toEndOf="@+id/btnAdminHome" />

    <TextView
        android:id="@+id/TNavAdOccassoin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/view_occasions"
        android:textColor="#0B0A0A"
        app:layout_constraintBaseline_toBaselineOf="@+id/TVNavAdOrder"
        app:layout_constraintEnd_toStartOf="@+id/btnAdminAccount"
        app:layout_constraintStart_toEndOf="@+id/btnAdminOrders" />

    <TextView
        android:id="@+id/TvNavAdAccount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/view_account"
        android:textColor="#070606"
        app:layout_constraintBottom_toBottomOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toEndOf="@+id/TVNavBarAdmin"
        app:layout_constraintHorizontal_bias="0.626"
        app:layout_constraintStart_toEndOf="@+id/btnAdminProducts"
        app:layout_constraintTop_toBottomOf="@+id/btnAdminAccount"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/TvadminProductMain"
        android:layout_width="380dp"
        android:layout_height="49dp"
        android:background="#FFFFFF"
        android:text="@string/view_products"
        android:textAlignment="center"
        android:textColor="#FF6F00"
        android:textSize="24sp"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toTopOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.516"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.049" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/TvadminProductMain"
        app:layout_constraintEnd_toEndOf="@+id/TvadminProductMain"
        app:layout_constraintHorizontal_bias="0.936"
        app:layout_constraintStart_toStartOf="@+id/TvadminProductMain"
        app:layout_constraintTop_toTopOf="@+id/TvadminProductMain"
        app:layout_constraintVertical_bias="0.437"
        app:srcCompat="@drawable/occasion" />

    <TextView
        android:id="@+id/TvAdminProductContainer"
        android:layout_width="377dp"
        android:layout_height="194dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TvadminProductMain"
        app:layout_constraintVertical_bias="0.072" />

    <TextView
        android:id="@+id/TvAdminProductName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/toblerone"
        android:textColor="#070606"
        android:textSize="18sp"
        android:textStyle="normal|bold|italic"
        app:layout_constraintBottom_toBottomOf="@+id/TvAdminProductContainer"
        app:layout_constraintEnd_toEndOf="@+id/TvAdminProductContainer"
        app:layout_constraintHorizontal_bias="0.069"
        app:layout_constraintStart_toStartOf="@+id/TvAdminProductContainer"
        app:layout_constraintTop_toTopOf="@+id/TvAdminProductContainer"
        app:layout_constraintVertical_bias="0.047" />

    <ImageView
        android:id="@+id/IvAdminProductImage"
        android:layout_width="153dp"
        android:layout_height="114dp"
        app:layout_constraintBottom_toBottomOf="@+id/TvAdminProductContainer"
        app:layout_constraintEnd_toEndOf="@+id/TvAdminProductContainer"
        app:layout_constraintHorizontal_bias="0.066"
        app:layout_constraintStart_toStartOf="@+id/TvAdminProductContainer"
        app:layout_constraintTop_toBottomOf="@+id/TvAdminProductName"
        app:layout_constraintVertical_bias="0.166"
        app:srcCompat="@drawable/toblerone" />

    <TextView
        android:id="@+id/TvAdminProductPrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/price"
        android:textColor="#070606"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toBottomOf="@+id/TvAdminProductContainer"
        app:layout_constraintEnd_toEndOf="@+id/TvAdminProductContainer"
        app:layout_constraintHorizontal_bias="0.267"
        app:layout_constraintStart_toEndOf="@+id/IvAdminProductImage"
        app:layout_constraintTop_toTopOf="@+id/TvAdminProductContainer"
        app:layout_constraintVertical_bias="0.348" />

    <Button
        android:id="@+id/btnAdminEditProduct"
        android:layout_width="134dp"
        android:layout_height="34dp"
        android:background="#BF360C"
        android:text="@string/editProd"
        android:textColor="#FFFFFF"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toBottomOf="@+id/TvAdminProductContainer"
        app:layout_constraintEnd_toEndOf="@+id/TvAdminProductContainer"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toEndOf="@+id/IvAdminProductImage"
        app:layout_constraintTop_toBottomOf="@+id/TvAdminProductPrice"
        app:layout_constraintVertical_bias="0.2" />

    <Button
        android:id="@+id/btnAdminProductRemove"
        android:layout_width="134dp"
        android:layout_height="34dp"
        android:background="#BF360C"
        android:text="@string/removeProd"
        android:textColor="#FFFFFF"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toBottomOf="@+id/TvAdminProductContainer"
        app:layout_constraintEnd_toEndOf="@+id/TvAdminProductContainer"
        app:layout_constraintStart_toEndOf="@+id/IvAdminProductImage"
        app:layout_constraintTop_toBottomOf="@+id/btnAdminEditProduct" />

    <Button
        android:id="@+id/btnAdminProductAddNew"
        android:layout_width="343dp"
        android:layout_height="48dp"
        android:background="#BF360C"
        android:text="@string/AddNew"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="normal|bold"
        app:layout_constraintBottom_toTopOf="@+id/TVNavBarAdmin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TvAdminProductContainer"
        app:layout_constraintVertical_bias="0.922" />


</androidx.constraintlayout.widget.ConstraintLayout>