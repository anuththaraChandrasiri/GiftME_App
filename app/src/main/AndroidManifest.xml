<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.madd_ordermanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="GiftME"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AdminMyAccount"></activity>
        <activity android:name=".CustomerMyAccount" />
        <activity android:name=".AdminViewOrder" />
        <activity android:name=".AdminOrders" />
        <activity android:name=".CancelOrder" />
        <activity android:name=".ViewOrder" />
        <activity android:name=".OrdersToPay" />
        <activity android:name=".MyAllOrders" />
        <activity android:name=".DeleteFromCart" />
        <activity android:name=".MyCart" />
        <activity android:name=".Checkout" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>